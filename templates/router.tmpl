package routers

import (
	"github.com/astaxie/beego"
	"github.com/{{.Import}}/controllers"
)

func init() {
	ns := beego.NewNamespace("/v1",
		{{range $i, $unit := .Units}}
		beego.NSNamespace("/{{.URL}}",
			beego.NSInclude(
				&controllers.{{.Name}}Controller{},
			),
		),{{end}}
	)
	beego.AddNamespace(ns)
}
